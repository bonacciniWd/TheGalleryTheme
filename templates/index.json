        "custom_liquid": "<div class=\"only-desktop\">\n<div class=\"before-after-slider\">\n  <div class=\"wrapper\">\n\n    <div class=\"before\">\n      <img class=\"content-image\" src=\"{{ 'before.png' | asset_url }}\" draggable=\"false\" alt=\"Before Image\" />\n    </div>\n\n    <div class=\"after\">\n      <img class=\"content-image\" src=\"{{ 'after.png' | asset_url }}\" draggable=\"false\" alt=\"After Image\" />\n    </div>\n\n    <button id=\"scroller\">\n      <div id=\"text\">AMATERASU</div>\n      <div id=\"icon-wrapper\">\n       <img id=\"star-svg\" src=\"{{ 'yinh.svg' | asset_url }}\" />\n      </div>\n    </button>\n\n  </div>\n</div>\n</div>\n\n<script>\n  const wrapper = document.querySelector('.before-after-slider .wrapper');\n  const after = wrapper.querySelector('.after');\n  const scroller = document.getElementById('scroller');\n  const iconWrapper = document.getElementById('icon-wrapper');\n  const starSVG = document.getElementById('star-svg');\n  const text = document.getElementById('text');\n\n  let toggled = false;\n  const duration = 600;\n  let currentRotation = 0;\n\n  // Atualiza posi√ß√µes e tamanhos conforme container\n  function updateLineAndScroller(clampedX) {\n    const scrollerWidth = scroller.offsetWidth;\n    const iconWidth = iconWrapper.offsetWidth;\n    const linePosition = clampedX + (scrollerWidth - iconWidth / 2);\n    const maxLineWidth = wrapper.offsetWidth;\n    const lineWidthClamped = Math.min(linePosition, maxLineWidth);\n\n    after.style.width = lineWidthClamped + \"px\";\n    scroller.style.left = (clampedX - (scrollerWidth - iconWidth / 2)) + \"px\";\n  }\n\n  // Anima√ß√£o da linha e rota√ß√£o\n  function animateLineAndRotateSVG(from, to, duration) {\n    let start = null;\n\n    function step(timestamp) {\n      if (!start) start = timestamp;\n      let progress = (timestamp - start) / duration;\n      if (progress > 1) progress = 1;\n\n      let currentX = from + (to - from) * progress;\n      updateLineAndScroller(currentX);\n\n      let rotationDeg = currentRotation + 360 * progress;\n      starSVG.style.transform = `rotate(${rotationDeg}deg)`;\n\n      if (progress < 1) {\n        requestAnimationFrame(step);\n      } else {\n        currentRotation += 360;\n        // Atualiza texto e posi√ß√£o ao fim da anima√ß√£o\n        if (toggled) {\n          text.textContent = \"COMPANY\";\n          scroller.classList.add('right');\n        } else {\n          text.textContent = \"AMATERASU\";\n          scroller.classList.remove('right');\n        }\n      }\n    }\n    requestAnimationFrame(step);\n  }\n\n  // Ajuste inicial e evento click\n  function init() {\n    // Posi√ß√£o inicial fixa\n    updateLineAndScroller(80);\n    text.textContent = \"AMATERASU\";\n    scroller.classList.remove('right');\n  }\n\n  scroller.addEventListener('click', () => {\n    toggled = !toggled;\n    const from = toggled ? 70 : wrapper.offsetWidth - (scroller.offsetWidth - iconWrapper.offsetWidth / 2);\n    const to = toggled ? wrapper.offsetWidth - (scroller.offsetWidth - iconWrapper.offsetWidth / 2) : 70;\n\n    animateLineAndRotateSVG(from, to, duration);\n  });\n\n  // Atualiza layout no resize\n  window.addEventListener('resize', () => {\n    if (toggled) {\n      updateLineAndScroller(wrapper.offsetWidth - (scroller.offsetWidth - iconWrapper.offsetWidth / 2));\n    } else {\n      updateLineAndScroller(70);\n    }\n  });\n\n  init();\n</script>",
        "color_scheme": "",
        "padding_top": 0,
        "padding_bottom": 0
      }
    }

      "settings": {
        "custom_liquid": "{% comment %} sections/promo-grid.liquid {% endcomment %}\n<div class=\"promo-grid page-width\">\n  <div class=\"promo-grid__container\">\n    {% for block in section.blocks %}\n      <div class=\"promo-card promo-card--{{ block.settings.size }}\" {{ block.shopify_attributes }}>\n        <div class=\"promo-card__image\">\n          {% if block.settings.image %}\n            {{ block.settings.image | image_url: width: 1500 | image_tag:\n              loading: 'lazy',\n              widths: '375, 550, 750, 1100, 1500',\n              class: 'promo-card__bg'\n            }}\n          {% endif %}\n        </div>\n        \n        <div class=\"promo-card__content\">\n          {% if block.settings.subtitle != blank %}\n            <p class=\"promo-card__subtitle\">{{ block.settings.subtitle }}</p>\n          {% endif %}\n          \n          {% if block.settings.title != blank %}\n            <h2 class=\"promo-card__title\">{{ block.settings.title }}</h2>\n          {% endif %}\n          \n          {% if block.settings.text != blank %}\n            <div class=\"promo-card__text\">{{ block.settings.text }}</div>\n          {% endif %}\n          \n          {% if block.settings.button_label != blank %}\n            <a href=\"{{ block.settings.button_link }}\" class=\"button promo-card__button\">\n              {{ block.settings.button_label }}\n            </a>\n          {% endif %}\n          \n          {% if block.settings.discount_code != blank %}\n            <div class=\"promo-card__code\">{{ block.settings.discount_code }}</div>\n          {% endif %}\n        </div>\n      </div>\n    {% endfor %}\n  </div>\n</div>\n\n<style>\n  .promo-grid {\n    padding: 4rem 2rem;\n  }\n\n  .promo-grid__container {\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));\n    gap: 2rem;\n  }\n\n  .promo-card {\n    position: relative;\n    border-radius: 12px;\n    overflow: hidden;\n    min-height: 400px;\n  }\n\n  .promo-card--large {\n    grid-column: span 2;\n  }\n\n  .promo-card__bg {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    position: absolute;\n    top: 0;\n    left: 0;\n    z-index: 1;\n  }\n\n  .promo-card__content {\n    position: relative;\n    z-index: 2;\n    padding: 3rem;\n    color: #fff;\n    height: 100%;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    background: linear-gradient(0deg, rgba(0,0,0,0.5) 0%, rgba(0,0,0,0.3) 100%);\n  }\n\n  .promo-card__subtitle {\n    font-size: 1.6rem;\n    margin-bottom: 1rem;\n  }\n\n  .promo-card__title {\n    font-size: 3.2rem;\n    margin: 0 0 2rem 0;\n  }\n\n  .promo-card__text {\n    font-size: 1.8rem;\n    margin-bottom: 2rem;\n  }\n\n  .promo-card__button {\n    align-self: flex-start;\n    background: #fff;\n    color: #000;\n    padding: 1.2rem 3rem;\n    text-decoration: none;\n    border-radius: 25px;\n    font-weight: 700;\n  }\n\n  .promo-card__code {\n    font-size: 1.4rem;\n    margin-top: 2rem;\n    background: rgba(255,255,255,0.2);\n    padding: 0.8rem 1.5rem;\n    border-radius: 4px;\n    display: inline-block;\n  }\n\n  @media (max-width: 749px) {\n    .promo-grid__container {\n      grid-template-columns: 1fr;\n    }\n    \n    .promo-card--large {\n      grid-column: span 1;\n    }\n  }\n</style>\n\n\n{\n  \"name\": \"Promo Grid\",\n  \"max_blocks\": 4,\n  \"blocks\": [\n    {\n      \"type\": \"promo\",\n      \"name\": \"Promo Item\",\n      \"settings\": [\n        {\n          \"type\": \"image_picker\",\n          \"id\": \"image\",\n          \"label\": \"Image\"\n        },\n        {\n          \"type\": \"select\",\n          \"id\": \"size\",\n          \"label\": \"Block size\",\n          \"options\": [\n            {\n              \"value\": \"normal\",\n              \"label\": \"Normal\"\n            },\n            {\n              \"value\": \"large\",\n              \"label\": \"Large\"\n            }\n          ],\n          \"default\": \"normal\"\n        },\n        {\n          \"type\": \"text\",\n          \"id\": \"subtitle\",\n          \"label\": \"Subtitle\",\n          \"default\": \"Popular Collections\"\n        },\n        {\n          \"type\": \"text\",\n          \"id\": \"title\",\n          \"label\": \"Title\",\n          \"default\": \"Valentine's\"\n        },\n        {\n          \"type\": \"richtext\",\n          \"id\": \"text\",\n          \"label\": \"Text\",\n          \"default\": \"<p>Up to 80% off</p>\"\n        },\n        {\n          \"type\": \"text\",\n          \"id\": \"button_label\",\n          \"label\": \"Button label\",\n          \"default\": \"Shop Now\"\n        },\n        {\n          \"type\": \"url\",\n          \"id\": \"button_link\",\n          \"label\": \"Button link\"\n        },\n        {\n          \"type\": \"text\",\n          \"id\": \"discount_code\",\n          \"label\": \"Discount code\",\n          \"default\": \"Discount Code: KIDSPLAY15\"\n        }\n      ]\n    }\n  ],\n  \"presets\": [\n    {\n      \"name\": \"Promo Grid\",\n      \"blocks\": [\n        {\n          \"type\": \"promo\",\n          \"settings\": {\n            \"size\": \"large\",\n            \"subtitle\": \"Popular Collections\",\n            \"title\": \"Valentine's\",\n            \"text\": \"<p>Up to 80% off</p>\",\n            \"button_label\": \"Everything →\"\n          }\n        },\n        {\n          \"type\": \"promo\",\n          \"settings\": {\n            \"subtitle\": \"\",\n            \"title\": \"House of Boxplay\",\n            \"text\": \"\",\n            \"button_label\": \"Shop Now\",\n            \"discount_code\": \"Discount Code: KIDSPLAY15\"\n          }\n        }\n      ]\n    }\n  ]\n}",

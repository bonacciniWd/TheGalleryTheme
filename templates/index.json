    "custom_liquid_YpPPeC": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% assign lambe_products = collections['all'].products %}\n\n{% if lambe_products.size > 0 %}\n  <div class=\"lambe-highlight\">\n    <div class=\"center\">\n      <div class=\"twinkling\"></div>\n\n      <div class=\"lambe-scroll-container\">\n        <div class=\"lambe-scroll\">\n          {% for product in lambe_products %}\n            {% for image in product.images %}\n              <div class=\"lambe-item\">\n                <img src=\"{{ image | img_url: 'large' }}\" alt=\"{{ product.title }}\" class=\"lambe-image\">\n              </div>\n            {% endfor %}\n          {% endfor %}\n          {% for product in lambe_products %}\n            {% for image in product.images %}\n              <div class=\"lambe-item\">\n                <img src=\"{{ image | img_url: 'large' }}\" alt=\"{{ product.title }}\" class=\"lambe-image\">\n              </div>\n            {% endfor %}\n          {% endfor %}\n        </div>\n      </div>\n\n     <a href=\"{{ collections['all'].url }}\" class=\"lambe-button\">\n  <img src=\"{{ 'fundinho.png' | asset_url }}\" alt=\"√çcone\" class=\"button-icon\">\n  Ver mais...„Ö§  \n</a>\n     \n\n      <!-- Camadas de Blur -->\n      <div class=\"blur-layer blur-layer1\"></div>\n      <div class=\"blur-layer blur-layer2\"></div>\n    </div>\n  </div>\n{% endif %}\n\n<style>\n  .lambe-highlight {\n    text-align: center;\n    background: url('{{ 'fundo.png' | asset_url }}') no-repeat center center;\n    background-size: cover;\n    padding: 80px 0px;\n    border-radius: 0px;\n    max-width: auto;\n    margin-top: -40px;\nheight: 100vh;\n    overflow: hidden;\n    position: relative;\n  }\n /* Estilos para a imagem abaixo do bot√£o */\n  .image-below-button {\n    text-align: center;\n    margin-top: 20px;\n  }\n\n  .centered-image {\n    width: 120px;\nmargin-top: 10px;\n    height: 120px;\n    object-fit: cover;\n    border-radius: 10px;\n animation: spin 5s infinite;\n  }\n\n  /* Responsivo */\n  @media (max-width: 768px) {\n    .centered-image {\n      width: 100px;\n      height: 100px;\n    }\n  }\n  /* Simula√ß√£o do PointLight */\n  .lambe-highlight::before {\n    content: \"\";\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    width: 600px;\n    height: 600px;\n    background: radial-gradient(circle, rgba(255, 255, 200, 0.7) 0%, rgba(0, 0, 0, 0) 70%);\n    transform: translate(-50%, -50%);\n    filter: blur(80px);\n    opacity: 0.6;\n    pointer-events: none;\n  }\n\n  /* Blur Effect */\n  .blur-layer {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 150%;\n    height: 100%;\n    background: radial-gradient(circle, rgba(255, 255, 255, 0.2) 10%, rgba(0, 0, 0, 0) 80%);\n    filter: blur(20px);\n    opacity: 0.4;\n    pointer-events: none;\n  }\n\n  .blur-layer1 {\n    animation: blurMove1 15s linear infinite alternate;\n  }\n\n  .blur-layer2 {\n    animation: blurMove2 25s linear infinite alternate-reverse;\n    opacity: 0.3;\n  }\n\n  @keyframes blurMove1 {\n    from {\n      transform: translateX(-10%) translateY(-5%);\n    }\n    to {\n      transform: translateX(10%) translateY(5%);\n    }\n  }\n\n  @keyframes blurMove2 {\n    from {\n      transform: translateX(5%) translateY(10%);\n    }\n    to {\n      transform: translateX(-5%) translateY(-10%);\n    }\n  }\n\n  .lambe-title {\n    font-size: 24px;\n    font-weight: bold;\n    margin-bottom: 30px;\n    color: #fff;\n    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\n  }\n\n .lambe-scroll-container {\n    width: 100%; /* Ocupa 100% da largura */\n    overflow: hidden;\n    position: relative;\n    white-space: nowrap;\n    padding: 0; /* Remove padding */\n    margin: 0; /* Remove margens */\n}\n\n .lambe-scroll {\n    display: flex;\n    gap: 80px; /* Espa√ßamento entre as imagens */\n    animation: scrollProducts 25s linear infinite;\n    width: max-content;\n    padding-left: 0; /* Remove padding √† esquerda */\n    padding-right: 0; /* Remove padding √† direita */\n    margin-left: 0; /* Remove margem √† esquerda */\n    margin-right: 0; /* Remove margem √† direita */\n}\n\n  .lambe-item {\n    flex: 0 0 auto;\n    width: 200px;\nmargin-top:20px;\n  }\n\n  .lambe-image {\n    width: auto;\n    height: 280px;\n    border-radius: 10px;\n    transition: transform 0.3s ease-in-out;\n  }\n\n  .lambe-image:hover {\n    transform: scale(1.10);\n  }\n\n  @keyframes scrollProducts {\n    from {\n      transform: translateX(0);\n    }\n    to {\n      transform: translateX(-50%);\n    }\n  }\n\n .lambe-button {\n    display: inline-flex; /* Layout flex√≠vel */\n    align-items: center; /* Centraliza verticalmente */\n    justify-content: center; /* Centraliza horizontalmente */\n    color: #fff;\n    padding: 12px 1.px;\n    font-size: 16px;\n    font-weight: 600;\n    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.5);\n    background: linear-gradient(45deg, #FF416C, #FF4B2B);\n    border: none;\n    border-radius: 30px;\n    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);\n    transition: all 0.3s ease-in-out;\n    cursor: pointer;\n    margin-top: 80px;\n    text-decoration: none;\n    opacity: 1;\n    gap: 10px; /* Espa√ßamento entre a imagem e o texto */\n  }\n\n  .lambe-button:hover {\n    background: linear-gradient(45deg, #FF4B2B, #FF416C);\n    transform: translateY(-3px);\n    box-shadow: 0px 6px 8px rgba(0, 0, 0, 0.3);\n    opacity: 0.8;\n  }\n\n  .lambe-button:active {\n    transform: translateY(0);\n    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);\n  }\n\n  .button-icon {\n    width: 45px; /* Tamanho da imagem */\n    height: 45px;\nanimation: spin 4s infinite;\n    object-fit: cover; /* Garante que a imagem cubra o espa√ßo sem distorcer */\n    background-color: #fff; \n    background-opacity: 0.5;\n    border-radius: 50%; /* Formato redondo */\n    padding: 0px; \n}\n.lambe-button:active {\n    transform: translateY(0); /* Remove o efeito de levantar ao clicar */\n    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2); /* Volta √† sombra original */\n}\n\n  /* Ajustes para desktop */\n  @media (min-width: 1024px) {\n\n   .lambe-image {\n    width: 400px;\n    height: auto;\n    border-radius: 10px;\n    transition: transform 0.3s ease-in-out;\n  }\n    .lambe-highlight {\n      padding: 100px 80px;\n    }\n\n    .lambe-title {\n      font-size: 24px;\n      font-weight: bold;\n      position: absolute;\n      top: 10px;\n      left: 50%;\n      transform: translateX(-50%);\n      color: #fff;\n      text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\n    }\n\n    .lambe-item {\n      width: 400px;\n    }\n\n    .lambe-scroll {\n      gap: 60px;\n      animation-duration: 30s;\n    }\n  }\n\n  @media (min-width: 1024px) {\n    .lambe-highlight {\n      padding: 100px 80px;\n      background-size: cover;\n    }\n  }\n@media (max-width: 768px) {\n    .lambe-button {\n      font-size: 18px; /* Tamanho menor para mobile */\n      padding: 10px 10px;\n    }\n\n    .button-icon {\n    width: 40px; /* Tamanho da imagem */\n    height: 40px;\n    object-fit: cover; /* Garante que a imagem cubra o espa√ßo sem distorcer */\n    background-color: #fff; \n    background-opacity: 0.2;\n    border-radius: 50%; /* Formato redondo */\n    padding: 0px; \nanimation: spin 4s infinite;\n}\n  }\n\n</style>",

idth: 100%;\n    height: 100%;\n    object-fit: cover;\n    transition: transform 0.3s ease, filter 0.3s ease; /* Adiciona transi√ß√£o suave */\n  }\n\n  .collection-title {\n    position: absolute;\n    bottom: 10px;\n    left: 10px;\n    color: white;\n    font-size: 1.2em;\n    background-color: rgba(0, 0, 0, 0.5);\n    padding: 5px 10px;\n    border-radius: 5px;\n    transition: transform 0.3s ease, font-size 0.3s ease; /* Adiciona transi√ß√£o suave */\n  }\n\n  /* Efeito hover */\n  .parent > div:hover img {\n    transform: scale(1.05); /* Aumenta ligeiramente a imagem */\n    filter: brightness(0.8); /* Escurece a imagem */\n  }\n\n  .parent > div:hover .collection-title {\n    transform: translateY(-5px); /* Move o t√≠tulo para cima */\n    font-size: 1.3em; /* Aumenta ligeiramente o tamanho do texto */\n  }\n\n  /* Estilos para mobile */\n  @media (max-width: 768px) {\n    .categories-title {\n      text-align: center; /* Centraliza o t√≠tulo no mobile */\n    }\n\n    .parent {\n      display: flex;\n      flex-direction: column; /* Exibe os itens em cascata (uma coluna) */\n      gap: 10px; /* Mant√©m o gap entre os itens */\n    }\n\n    .parent > div {\n      width: 100%; /* Faz cada item ocupar 100% da largura */\n      height: auto;\n      border-radius: 10px; /* Arredondamento de borda no mobile */\n      grid-area: auto !important; /* Remove o posicionamento do grid */\n    }\n\n    .parent img {\n      height: 200px; /* Define uma altura fixa para as imagens no mobile */\n    }\n\n    .wrapper {\n      width: 100%;\n      height: auto; /* Ajuste conforme necess√°rio */\n      padding: 20px;\n    }\n  }\n</style>",

    "custom_liquid_YpPPeC": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% assign lambe_products = collections['mais-vendidos'].products %}\n\n{% if lambe_products.size > 0 %}\n  <div class=\"lambe-highlight\">\n    \n    \n    <div class=\"lambe-scroll-container\">\n      <div class=\"lambe-scroll\">\n        {% for product in lambe_products %}\n          {% for image in product.images %}\n            <div class=\"lambe-item\">\n              <img src=\"{{ image | img_url: 'large' }}\" alt=\"{{ product.title }}\" class=\"lambe-image\">\n            </div>\n          {% endfor %}\n        {% endfor %}\n        {% for product in lambe_products %}\n          {% for image in product.images %}\n            <div class=\"lambe-item\">\n              <img src=\"{{ image | img_url: 'large' }}\" alt=\"{{ product.title }}\" class=\"lambe-image\">\n            </div>\n          {% endfor %}\n        {% endfor %}\n      </div>\n    </div>\n\n    <a href=\"{{ collections['lambe-lambes'].url }}\" class=\"lambe-button\">Ver coleção</a>\n\n    <!-- Camadas de Blur -->\n    <div class=\"blur-layer blur-layer1\"></div>\n    <div class=\"blur-layer blur-layer2\"></div>\n  </div>\n{% endif %}\n\n<style>\n  .lambe-highlight {\n    text-align: center;\n    background: url('{{ 'fundo.jpg' | asset_url }}') no-repeat center center;\n    background-size: cover;\n    padding: 80px 20px;\n    border-radius: 0px;\n    max-width: 1400px;\n    margin-top: -40px;\n    overflow: hidden;\n    position: relative;\n  }\n\n  /* Simulação do PointLight */\n  .lambe-highlight::before {\n    content: \"\";\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    width: 600px;\n    height: 600px;\n    background: radial-gradient(circle, rgba(255, 255, 200, 0.7) 0%, rgba(0, 0, 0, 0) 70%);\n    transform: translate(-50%, -50%);\n    filter: blur(80px);\n    opacity: 0.6;\n    pointer-events: none;\n  }\n\n  /* Blur Effect */\n  .blur-layer {\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 150%;\n    height: 100%;\n    background: radial-gradient(circle, rgba(255, 255, 255, 0.2) 10%, rgba(0, 0, 0, 0) 80%);\n    filter: blur(20px);\n    opacity: 0.4;\n    pointer-events: none;\n  }\n\n  .blur-layer1 {\n    animation: blurMove1 15s linear infinite alternate;\n  }\n\n  .blur-layer2 {\n    animation: blurMove2 25s linear infinite alternate-reverse;\n    opacity: 0.3;\n  }\n\n  @keyframes blurMove1 {\n    from {\n      transform: translateX(-10%) translateY(-5%);\n    }\n    to {\n      transform: translateX(10%) translateY(5%);\n    }\n  }\n\n  @keyframes blurMove2 {\n    from {\n      transform: translateX(5%) translateY(10%);\n    }\n    to {\n      transform: translateX(-5%) translateY(-10%);\n    }\n  }\n\n  .lambe-title {\n    font-size: 24px;\n    font-weight: bold;\n    margin-bottom: 30px;\n    color: #fff;\n    text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\n  }\n\n  .lambe-scroll-container {\n    width: 100%;\n    overflow: hidden;\n    position: relative;\n    white-space: nowrap;\n    padding: 20px 0;\n  }\n\n  .lambe-scroll {\n    display: flex;\n    gap: 40px;\n    animation: scrollProducts 25s linear infinite;\n    width: max-content;\n  }\n\n  .lambe-item {\n    flex: 0 0 auto;\n    width: 200px;\n  }\n\n  .lambe-image {\n    width: 100%;\n    height: auto;\n    border-radius: 10px;\n    \n    transition: transform 0.3s ease-in-out;\n  }\n\n  .lambe-image:hover {\n    transform: scale(1.05);\n  }\n\n  @keyframes scrollProducts {\n    from {\n      transform: translateX(0);\n    }\n    to {\n      transform: translateX(-50%);\n    }\n  }\n\n .lambe-button {\n    display: inline-block;\n    color: #fff; /* Texto branco */\n    padding: 10px 25px;\n    font-size: 16px;\n    font-weight: bolder;\n    text-shadow: 3px 3px 5px rgba(0, 0, 0, 0.8); /* Sombra preta no texto */\n    transition: text-shadow 0.3s ease-in-out;\n    background: none; /* Sem fundo */\n    border: none;\n    margin-top: 30px;\n}\n\n.lambe-button:hover {\n    color: #FFD700;\n}\n\n  /* Ajustes para desktop */\n  @media (min-width: 1024px) {\n    .lambe-highlight {\n      padding: 100px 80px;\n    }\n\n    .lambe-title {\n  font-size: 24px;\n  font-weight: bold;\n  position: absolute;\n  top: 10px; /* Apenas 10px do topo */\n  left: 50%;\n  transform: translateX(-50%);\n  color: #fff;\n  text-shadow: 2px 2px 5px rgba(0, 0, 0, 0.5);\n}\n\n\n    .lambe-item {\n      width: 300px;\n    }\n\n    .lambe-scroll {\n      gap: 60px;\n      animation-duration: 30s;\n    }\n  }\n@media (min-width: 1024px) {\n  .lambe-highlight {\n    padding: 100px 80px;\n    background-size: cover; /* Garante que cobre tudo */\n  }\n}\n</style>",
